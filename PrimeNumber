import java.lang.*;
public class Solution {
    public int isPrime(int A) {
        if(A==1)
        {
            return 0;
        }
        int n=(int)(Math.sqrt(A));
        for(int i=2;i<=n;i++)
        {
            if(A%i==0)
            {
                return 0;
            }
        }
        return 1;
    }
}
